#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
class Node
{
    public:
    int data;
    Node *next;
};
void push(Node** head_ref, int new_data)
{
    Node* new_node = new Node();
    new_node->data = new_data;
    new_node->next = (*head_ref);
    (*head_ref) = new_node;
}
int smallestElement(struct Node* head)
{
    int INT_MAX;
    int min = INT_MAX;
    while (head != NULL) {
        if (min > head->data)
            min = head->data;
 
        head = head->next;
    }
    cout<<min;
    return 0;
}
 
int main()
{
    Node* head = NULL;
    int n;
    for(int i=0;i<5;i++){
        cin>>n;
        push(&head,n);
    }
    smallestElement(head);
    return 0;
}
